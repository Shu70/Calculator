n1 = float(input('Enter first number:-'))
print('Choose Your Operations:-')
print('If Addition:-',"Press'+'")
print('If Substraction:-',"Press'-'")
print('If Multiplication:-',"Press'*'")
print('If Division:-',"Press'/'")
choice = input()
n2 = float(input('Enter second number:-'))
if choice is '+':
  print(n1+n2)
if choice is '-':
  print(n1-n2)
if choice is '*':
  print(n1*n2)
if choice is '/':
  print(n1/n2)
